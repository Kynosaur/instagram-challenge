<%= link_to 'Sign up',  new_user_path %>
<%= link_to 'Log in',   login_path %>
<%= link_to 'Log out',  logout_path %>

<% @posts.reverse.each do |post| %>
  <div class='post'>
    <img src="<%= post.picture.url %>">
    <p>
    <%= simple_format(post.caption) %>
    </p>
  </div>
<% end %>

<%= link_to 'New post', new_post_path %>
